<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Pair3" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#C8B8EC</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="MyDefaultTextView">
        <item name="android:layout_width">@dimen/_25sdp</item>
        <item name="android:layout_height">@dimen/_25sdp</item>
        <!-- Ajoutez d'autres propriétés par défaut ici -->
    </style>

    <!-- res/values/rounded_seekbar_style.xml -->
    <style name="RoundedSeekBarStyle" parent="Widget.AppCompat.SeekBar">
        <item name="android:progressDrawable">@drawable/rounded_seekbar_progress</item>

    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSize50Percent" parent="">
    <item name="cornerSize">50%</item>
    </style>


</resources>